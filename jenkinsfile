pipeline {
    agent any
        tools {
            nodejs("24.6.0")
        }
        stages {
            stage("checkout") {
            steps {
                checkout scm
                   }
            }

                  stage("test") {
                        steps {
                           sh 'npm version'
                           sh 'npm install'
                           sh 'npm test'
                        }
                        }


          stage("build") {
                steps {
                    sh 'npm run dev'
                }
                }
    }
}
